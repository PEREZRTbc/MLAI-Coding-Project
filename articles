import requests
import pandas as pd
API_KEY = "d6c772a3-c100-49b4-88f2-87c130e4bca0"
url = f"https://api.goperigon.com/v1/all?companySymbol=AAPL&from=2022-03-01&to=2022-03-10&apiKey={API_KEY}"

resp = requests.get(url)
print(resp)
data = resp.json()
data = data['articles']
print(data)
df = pd.DataFrame(data)
print(df.head())

print(df.describe())
